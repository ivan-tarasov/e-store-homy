<div class="panel panel-faq">
	<div class="panel-heading" role="tab" id="heading-{faq_number}">
		<h4 class="panel-title">
			<a data-toggle="collapse" data-parent="#questions" class="collapsed" href="#collapse-{faq_number}">
				{faq_question}
			</a>
		</h4>
	</div>
	<div id="collapse-{faq_number}" class="panel-collapse collapse">
		<div class="callout callout-main">
			{faq_answer}
		</div>
	</div>
</div><!-- /.panel-faq -->